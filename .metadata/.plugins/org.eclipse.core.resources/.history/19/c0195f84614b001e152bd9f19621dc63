#ifndef SRC_PACKAGE_PACKAGE_H_
#define SRC_PACKAGE_PACKAGE_H_
#include<handshake/handshake.h>

typedef struct {
	int size,
	void* stream
} t_buffer;

typedef struct {
	enum MODULOS op_code,
	t_buffer buffer
} t_package;

void create_package();
void send_package();
void recieve_package();
static void create_buffer();

#endif /* SRC_PACKAGE_PACKAGE_H_ */
